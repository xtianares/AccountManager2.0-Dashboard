  <meta name="description" content="{{PageDescription}}" />
  <meta name="theme-color" content="#0098DA" />
  <link rel="manifest" href="/manifest.json" />
  <link rel="shortcut icon" type="image/x-icon" href="/images/american-safety-council-colored.png" />
  <link rel="canonical" href="https://www.mydomain.com{{CanonicalPage}}">
