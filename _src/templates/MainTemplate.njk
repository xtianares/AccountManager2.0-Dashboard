<header id="header-wrapper" class="clearfix">
  {% include "./partials/Navbar.njk" %}
</header> <!-- end #header_wrapper -->

{% block MainContent %}{% endblock %}

<footer id="footer-wrap" class="footer mt-auto">
  <div class="container">
    <div class="row">
      <div class="col-sm-12">
        <p class="copyright text-center mb-0">Copyright &#169; <script id="year_display">var y=new Date().getFullYear();var s=document.getElementById("year_display");s.parentNode.replaceChild(document.createTextNode(y),s);</script>, American Safety Council. All Rights Reserved.</p>
      </div>
    </div>
  </div>
</footer>

